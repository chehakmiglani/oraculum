{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "framework": "vite",
  "env": {
    "VITE_API_BASE_URL": ""
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://oraculum-backend.chehakmiglani.workers.dev/$1"
    }
  ]
}